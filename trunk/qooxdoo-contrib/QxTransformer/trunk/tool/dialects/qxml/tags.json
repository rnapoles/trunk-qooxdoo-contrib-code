            }
        },
        "attributes": {
          //custom attributes
           "selectionMode" : "attr/qx/table/attribute.mako",
           "tableModel":"",
           "treeDataCellRenderer":"",
           "defaultDataCellRenderer":"",
           "dataRowRenderer":"",
           "selectionManager":"",
           "tableColumnModel":""
      }        
    },
    
