{
  "name"    : "databinding",

  "include" :
  [
    {
      "path" : "${QOOXDOO_PATH}/tool/data/config/application.json"
    }
  ],

  "export" :
  [
    "api",
    "build",
    "clean",
    "distclean",
    "fix",
    "inspector",
    "lint",
    "migration",
    "pretty",
    "profiling",
    "source",
    "source-all",
    "test",
    "test-source",
    "translation"
  ],

  "let" :
  {
    "APPLICATION"  : "databinding",
    "QOOXDOO_PATH" : "../../../../../qooxdoo-trunk",
    "QXTHEME"      : "databinding.theme.Theme",
    "API_EXCLUDE"  : ["qx.legacy.*","qx.test.*"],
    "LOCALES"      : [ "en","de" ],
    "ROOT"         : "."
  },

  "jobs" :
  {
    "libraries" : 
    {
      "library" :
      [
        {
          "manifest" : "contrib://qcl/trunk/frontend/Manifest.json"
        }
      ]
    }
  }
}
