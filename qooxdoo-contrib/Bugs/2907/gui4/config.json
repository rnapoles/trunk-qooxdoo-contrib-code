{
  "name"    : "gui4",

  "include" :
  [
    {
      "path" : "${QOOXDOO_PATH}/tool/data/config/application.json"
    }
  ],

  "export" :
  [
    "api",
    "build",
    "clean",
    "distclean",
    "fix",
    "info",
    "inspector",
    "lint",
    "migration",
    "pretty",
    "profiling",
    "source",
    "source-all",
    "test",
    "test-source",
    "translation"
  ],

  "let" :
  {
    "APPLICATION"  : "gui4",
    "QOOXDOO_PATH" : "../../../qooxdoo/trunk",
    "QXTHEME"      : "gui4.theme.Theme",
    "API_EXCLUDE"  : ["qx.test.*"],
    "LOCALES"      : [ "en" ],
    "CACHE"        : "${TMPDIR}/cache",
    "ROOT"         : "."
  }

  ,"jobs" :
  {
    "libraries" : 
    {
      "library" :
      [
        {
          "manifest" : "../../../SkeletonWidget/trunk/Manifest.json"
        }
      ]
    }
  }
}
